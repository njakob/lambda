{
  "name": "@njakob/lambda",
  "version": "1.0.1",
  "description": "Utils to deploy AWS Lambda functions written in Javascript",
  "author": "Nicolas Jakob <nicow.jakob@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/njakob/lambda",
  "repository": {
    "type": "git",
    "url": "git://github.com/njakob/lambda.git"
  },
  "bugs": {
    "url": "https://github.com/njakob/lambda/issues"
  },
  "keywords": [
    "aws",
    "lambda",
    "pack",
    "build",
    "bundle",
    "tool",
    "cli",
    "command-line"
  ],
  "bin": {
    "lambda": "bin/lambda.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build:source": "babel -d lib src --source-maps",
    "build:flow": "flow gen-flow-files --out-dir lib src/lambda/index.js",
    "build": "npm run clean && npm run build:source",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "flow": "flow check",
    "test": "npm run flow && npm run lint",
    "release": "standard-version",
    "prepublish": "npm run test"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.12.1",
    "eslint-config-njakob": "^4.1.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-flowtype": "^2.18.2",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.43.1",
    "rimraf": "^2.5.4",
    "standard-version": "^4.0.0"
  },
  "dependencies": {
    "@njakob/cli-utils": "^1.1.1",
    "@njakob/parcel": "^1.0.1",
    "archiver": "^1.3.0",
    "aws-sdk": "^2.28.0",
    "babel-runtime": "^6.23.0",
    "bugsy": "^2.1.0",
    "dotenv": "^4.0.0",
    "glob": "^7.1.1",
    "pretty-bytes": "^4.0.2",
    "raclette": "^1.0.2",
    "source-map-support": "^0.4.14",
    "stream-buffers": "^3.0.1",
    "yargs": "^7.1.0"
  }
}
